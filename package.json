  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2 start server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "prod": "cross-env NODE_ENV=production nodemon server.js",
    "seed": "npx sequelize-cli db:seed:all",
    "migrate": "npx sequelize-cli db:migrate",
    "clean-migrations": "npx sequelize db:migrate:undo:all",
    "clean-seed": "npx sequelize db:seed:undo:all",
    "clean-all": "npm run clean-seed && npm run clean-migrations",
    "fulldb": "npm run migrate && npm run seed",
    "fresh": "npm run clean-all && npm run fulldb",
    "mocha": "cross-env NODE_ENV=test nyc --reporter=html mocha --timeout=20000 --exit --reporter mochawesome",
    "test": "npm run fresh && npm run mocha"
  },
